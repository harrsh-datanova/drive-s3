        "format:fix": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,md}\""
    },
    "license": "MIT",
    "dependencies": {
        "concurrently": "^9.1.2"
    },
    "devDependencies": {
        "prettier": "^3.4.2"
    }
}
