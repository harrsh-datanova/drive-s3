{
    "name": "drive-s3",
    "version": "1.0.0",
    "main": "app/server.ts",
    "scripts": {
        "client": "npm run dev --prefix ./client",
        "server": "npm run dev --prefix ./server",
        "dev": "concurrently \"npm run dev --prefix ./client\" \"npm run dev --prefix ./server\"",
